{
  "name": "ember-qunit-nice-errors",
  "version": "1.2.1",
  "description": "Because expected true, result false is not enough!",
  "keywords": [
    "assertion",
    "ember",
    "ember-addon",
    "ember-cli",
    "error",
    "message",
    "qunit",
    "test"
  ],
  "homepage": "https://github.com/adopted-ember-addons/ember-qunit-nice-errors#readme",
  "bugs": "https://github.com/adopted-ember-addons/ember-qunit-nice-errors/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/adopted-ember-addons/ember-qunit-nice-errors.git"
  },
  "license": "MIT",
  "author": "WyeWorks",
  "contributors": [
    "Samanta de Barros <samanta.debarros.cejas@gmail.com> (https://github.com/sdebarros)",
    "Federico Kauffman <fede.kau@gmail.com> (https://github.com/fedekau)",
    "Diego Acosta <acostami@gmail.com> (https://github.com/acostami)",
    "Santiago Ferreira <san650@gmail.com> (https://github.com/san650)",
    "Robbie Wagner <rwwagner90@gmail.com> (https://github.com/RobbieTheWagner)"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "ember-test": "ember try:each",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "node-test": "mocha --inline-diffs --colors node-tests/*-test.js",
    "start": "ember serve",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "broccoli-persistent-filter": "^2.3.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-template-imports": "^4.3.0",
    "minimatch": "^10.0.3",
    "recast": "^0.23.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.2.2",
    "@embroider/macros": "^1.18.1",
    "@embroider/test-setup": "^4.0.0",
    "@eslint/js": "^9.32.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-test-helpers": "0.0.9",
    "chai": "^5.2.1",
    "concurrently": "^9.2.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.5.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deprecation-workflow": "^3.4.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.5.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^7.9.1",
    "ember-try": "^4.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.0",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-qunit": "^8.2.4",
    "globals": "^16.3.0",
    "loader.js": "^4.7.0",
    "mocha": "^11.7.1",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.1.0",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "release-plan": "^0.17.0",
    "stylelint": "^16.22.0",
    "stylelint-config-standard": "^38.0.0",
    "webpack": "^5.100.2"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "packageManager": "pnpm@10.14.0-0+sha512.2cd47a0cbf5f1d1de7693a88307a0ede5be94e0d3b34853d800ee775efbea0650cb562b77605ec80bc8d925f5cd27c4dfe8bb04d3a0b76090784c664450d32d6",
  "engines": {
    "node": ">= 20",
    "pnpm": ">= 10.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
