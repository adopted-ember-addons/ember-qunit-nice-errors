{
  "name": "ember-qunit-nice-errors",
  "version": "1.2.0",
  "description": "Because expected true, result false is not enough!",
  "keywords": [
    "assertion",
    "ember",
    "ember-addon",
    "ember-cli",
    "error",
    "message",
    "qunit",
    "test"
  ],
  "homepage": "https://github.com/wyeworks/ember-qunit-nice-errors#readme",
  "bugs": "https://github.com/wyeworks/ember-qunit-nice-errors/issues",
  "license": "MIT",
  "author": "WyeWorks",
  "contributors": [
    "Samanta de Barros <samanta.debarros.cejas@gmail.com> (https://github.com/sdebarros)",
    "Federico Kauffman <fede.kau@gmail.com> (https://github.com/fedekau)",
    "Diego Acosta <acostami@gmail.com> (https://github.com/acostami)",
    "Santiago Ferreira <san650@gmail.com> (https://github.com/san650)"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wyeworks/ember-qunit-nice-errors.git"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "ember-test": "ember try:each",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "node-test": "mocha --inline-diffs --colors node-tests/*-test.js",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "broccoli-persistent-filter": "^1.4.6",
    "ember-cli-babel": "^7.26.8",
    "ember-cli-htmlbars": "^6.0.1",
    "minimatch": "^3.0.4",
    "recast": "^0.13.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-test-helpers": "0.0.9",
    "chai": "^3.5.0",
    "ember-auto-import": "^2.2.4",
    "ember-cli": "~4.0.1",
    "ember-cli-addon-tests": "^0.11.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.0.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^2.0.0",
    "ember-welcome-page": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "mocha": "^6.1.4",
    "mocha-eslint": "~5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.65.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=2.4"
    }
  }
}
